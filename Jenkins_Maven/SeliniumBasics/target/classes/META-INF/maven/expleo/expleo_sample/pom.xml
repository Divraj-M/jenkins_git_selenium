<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>expleo</groupId>
  <artifactId>expleo_sample</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>expleo_sample</name>
  <url>http://www.example.com</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.release>17</maven.compiler.release>
    <selenium.version>4.27.0</selenium.version>
  </properties>

  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.junit</groupId>
        <artifactId>junit-bom</artifactId>
        <version>5.11.0</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <dependencies>
    <dependency>
      <groupId>commons-io</groupId>
      <artifactId>commons-io</artifactId>
      <version>2.18.0</version>
    </dependency>
    <dependency>
      <groupId>org.seleniumhq.selenium</groupId>
      <artifactId>selenium-java</artifactId>
      <version>${selenium.version}</version>
    </dependency>
    <dependency>
      <groupId>io.github.bonigarcia</groupId>
      <artifactId>webdrivermanager</artifactId>
      <version>5.8.0</version>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-api</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-params</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.codehaus.mojo</groupId>
      <artifactId>exec-maven-plugin</artifactId>
      <version>3.1.0</version>
    </dependency>
  </dependencies>

  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-clean-plugin</artifactId>
          <version>3.4.0</version>
        </plugin>
        <plugin>
          <artifactId>maven-resources-plugin</artifactId>
          <version>3.3.1</version>
        </plugin>
        <plugin>
          <artifactId>maven-compiler-plugin</artifactId>
          <version>3.13.0</version>
        </plugin>
        <plugin>
          <artifactId>maven-surefire-plugin</artifactId>
          <version>3.3.0</version>
        </plugin>
        <plugin>
          <artifactId>maven-jar-plugin</artifactId>
          <version>3.4.2</version>
        </plugin>
        <plugin>
          <artifactId>maven-install-plugin</artifactId>
          <version>3.1.2</version>
        </plugin>
        <plugin>
          <artifactId>maven-deploy-plugin</artifactId>
          <version>3.1.2</version>
        </plugin>
        <plugin>
          <artifactId>maven-site-plugin</artifactId>
          <version>3.12.1</version>
        </plugin>
        <plugin>
          <artifactId>maven-project-info-reports-plugin</artifactId>
          <version>3.6.1</version>
        </plugin>
      </plugins>
    </pluginManagement>

    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>exec-maven-plugin</artifactId>
        <version>3.1.0</version>
        <executions>
          <execution>
            <goals>
              <goal>java</goal>
            </goals>
            <configuration>
            <mainClass>BasicsSelinium.amazonDom</mainClass>
            <mainClass>BasicsSelinium.Actionclass1203</mainClass>
              <mainClass>BasicsSelinium.ExplicitWait</mainClass>
              <mainClass>BasicsSelinium.Multiclick1203</mainClass>
              <mainClass>BasicsSelinium.DropdownSelect1203</mainClass>
              <mainClass>BasicsSelinium.Task21203</mainClass>
              <mainClass>BasicsSelinium.Omayoaction</mainClass>
              <mainClass>BasicsSelinium.web1</mainClass>
              <mainClass>BasicsSelinium.Task2for0403</mainClass>
              <mainClass>BasicsSelinium.Task1for0403</mainClass>
              <mainClass>BasicsSelinium.Task1103</mainClass>
              <mainClass>BasicsSelinium.Task0703dropdown</mainClass>
              <mainClass>BasicsSelinium.ScreenShotmethods</mainClass>
              <mainClass>BasicsSelinium.RelativeLocator0703</mainClass>
              <mainClass>BasicsSelinium.OrangeHRM0703</mainClass>
              <mainClass>BasicsSelinium.OpenGoogle</mainClass>
              <mainClass>BasicsSelinium.Mobiles1</mainClass>
              <mainClass>BasicsSelinium.LeafGroundActions1203</mainClass>
              <mainClass>BasicsSelinium.File</mainClass>
              <mainClass>BasicsSelinium.KeyBoardaction</mainClass>
              <mainClass>BasicsSelinium.Omayoblog1303</mainClass>
              <mainClass>BasicsSelinium.DragandDrop1303</mainClass>
              <mainClass>BasicsSelinium.NewWindowV41303</mainClass>
              <mainClass>BasicsSelinium.omayoblogpopup1303</mainClass>
              <mainClass>BasicsSelinium.WindowsDemo</mainClass>
              <mainClass>BasicsSelinium.LeafGround1303Switch</mainClass>
              <mainClass>BasicsSelinium.Alert1403</mainClass>
              <mainClass>BasicsSelinium.LeafGroundFrames1403</mainClass>
              <mainClass>BasicsSelinium.Frame1403</mainClass>
              <mainClass>BasicsSelinium.Table1403static</mainClass>
              <mainClass>BasicsSelinium.CalenderHandle1403</mainClass>


               <!-- Replace with actual main class -->
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>